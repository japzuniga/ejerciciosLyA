Proceso promedio
	definir nota1, nota2, nota3, nota4, nota5, avg como real;
	
	escribir "cuanto saco en su primera clase?";
	leer nota1;
	escribir "cuanto saco en su segunda clase?";
	leer nota2;
	escribir "cuanto saco en su tercera clase?";
	leer nota3;
	escribir "cuanto saco en su cuarta clase?";
	leer nota4;
	escribir "cuanto saco en su quinta clase?";
	leer nota5;
	
	avg <- (nota1+nota2+nota3+nota4+nota5)/5;
	escribir "su promedio del semestre fue ", avg;
FinProceso
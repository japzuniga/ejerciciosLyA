Proceso salarioMensual
	definir pagoMensual, horasTrabajadas, salarioHora como entero;
	
	escribir "cuantas horas trabajo?";
	leer horasTrabajadas;
	escribir "cuanto le pagan por hora?";
	leer salarioHora;
	
	pagoMensual <- horasTrabajadas * salarioHora;
	escribir "su salario mensual es ", pagoMensual;	
FinProceso

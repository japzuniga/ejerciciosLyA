Algoritmo div
	//entradas
	definir num1 como real;
	definir num2 como real;
	definir resultado como real;
	
	//proceso
	escribir "dime el primer numero: ";
	leer num1;
	escribir "dime el segundo numero: ";
	leer num2;
	
	//salida
	resultado <- num1 / num2;
	escribir "la división es ", resultado;
FinAlgoritmo